{
    "name": "goetas/xsd2php",
    "description": "Convert XSD  (XML Schema) definitions into PHP classes",
    "type": "library",
    "authors": [
        {
            "name": "Asmir Mustafic"
        }
    ],
    "keywords": [
        "converter",
        "xml",
        "xsd",
        "php",
        "jms",
        "serializer"
    ],
    "license": "LGPL",
    "require": {
        "php": "~5.4",
        "symfony/console": "~2.1",
        "symfony/yaml": "~2.1",
        "goetas/xsd-reader": ">=1.0.1,<2.0",
        "doctrine/inflector": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Goetas\\Xsd\\XsdToPhp\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goetas\\Xsd\\XsdToPhp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
