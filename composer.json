{
  "name": "goetas-webservices/xsd2php",
  "description": "Convert XSD  (XML Schema) definitions into PHP classes and JMS metadata",
  "type": "library",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email" : "goetas@gmail.com"
    }
  ],
  "keywords": [
    "converter",
    "xml",
    "xsd",
    "php",
    "jms",
    "serializer"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2|^8.0",
    "symfony/console": "^2.7|^3.0|^4.0|^5.0|^6.0",
    "symfony/dependency-injection": "^2.2|^3.0|^4.0|^5.0|^6.0",
    "symfony/yaml": "^2.2|^3.0|^4.0|^5.0|^6.0",
    "symfony/config": "^2.2|^3.0|^4.0|^5.0|^6.0",
    "goetas-webservices/xsd-reader": "^0.3.7 | ^0.4.1",
    "doctrine/inflector": "^2.0",
    "laminas/laminas-code": "^3.3.2|^4.0",
    "psr/log": "^1.0 | ^2.0 | ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0",
    "jms/serializer": "^1.9|^2.0|^3.0",
    "goetas-webservices/xsd2php-runtime": "^0.2.13@dev",
    "symfony/validator": "^2.3.24|^3.0|^4.0|^5.0|^6.0",
    "dms/phpunit-arraysubset-asserts": "^0.3.1"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  },
  "bin": [
    "bin/xsd2php"
  ]
}
